atom ⊥  ;; false
atom ⊤  ;; true

rule Equality
  @b = @b → ⊤
  @b = @c → ⊥

rule Negation
  ¬⊤ → ⊥
  ¬⊥ → ⊤

rule Conjunction
  ⊥ ∧ ⊥ → ⊥
  ⊤ ∧ @b → @b
  @b ∧ ⊤ → @b

rule Disjunction
  ⊤ ∨ ⊤ → ⊤
  ⊥ ∨ @b → @b
  @b ∨ ⊥ → @b

rule ExclusiveDisjunction
  @a ⊻ @b → ¬{@a = @b}

rule Implication
  @a -> @b → ¬@a ∨ @b
  @a <-> @b → { @a -> @b } ∧ { @b -> @a } 
